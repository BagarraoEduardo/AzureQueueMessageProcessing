{
  "openapi": "3.0.1",
  "info": {
    "title": "ProcessorAPI",
    "version": "1.0"
  },
  "paths": {
    "/Processor": {
      "post": {
        "tags": [
          "Processor"
        ],
        "operationId": "Insert",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/ParsedTransferDTO"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/ParsedTransferDTO"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/ParsedTransferDTO"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/InsertParsedTransferResponseDTO"
                }
              }
            }
          },
          "500": {
            "description": "Server Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/InsertParsedTransferResponseDTO"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "InsertParsedTransferResponseDTO": {
        "type": "object",
        "additionalProperties": false,
        "properties": {
          "success": {
            "type": "boolean"
          },
          "errorMessage": {
            "type": "string",
            "nullable": true
          }
        }
      },
      "ParsedTransferDTO": {
        "type": "object",
        "additionalProperties": false,
        "properties": {
          "reference": {
            "type": "string",
            "format": "uuid"
          },
          "from": {
            "type": "string",
            "nullable": true
          },
          "to": {
            "type": "string",
            "nullable": true
          },
          "amount": {
            "type": "number",
            "format": "double"
          },
          "currency": {
            "type": "string",
            "nullable": true
          }
        }
      }
    }
  }
}